{
    "name": "tipoff/bookings",
    "description": "Laravel Package for Ecommerce Bookings",
    "keywords": [
        "tipoff",
        "bookings"
    ],
    "homepage": "https://github.com/tipoff/bookings",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "dniccum/phone-number": "^1.1",
        "inspheric/nova-email-field": "^1.42",
        "tipoff/authorization": "^2.7.1",
        "tipoff/statuses": "^2.1.0",
        "tipoff/support": "^1.8.8"
    },
    "require-dev": {
        "tipoff/test-support": "^1.4.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tipoff\\Bookings\\": "src",
            "Tipoff\\Bookings\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\Bookings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\Bookings\\BookingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
